                                        </div>
                                        <div class="txt-fld">
                                            <label for="">{IFOFFICE}</label>
                                            <input type="text"  required class="inputs" name="office" id="rg_office" placeholder="{RPOFFICE}" value="{OFFICEUSER}" />
