                                        <div class="txt-fld">
                                            <label for="">{IFNUMBERHOUSE}</label> 
                                            <input type="text" required class="inputs" name="numberhouse" id="rg_numberhouse" placeholder="{RPNUMBERHOUSE}" value="{NUMBERHOUSEUSER}" />
                                        </div>
                                        <div class="txt-fld">
                                            <label for="">{IFNOTEPOSITON}</label>
                                            <textarea name="noteposition" class="inputs" id="rg_noteposition" placeholder="{RPNOTEPOSITION}"  >{NOTEPOSITIONUSER}</textarea>
                                        </div>
                                    </div>

						            <div id="form-right">
						                <div class="txt-fld">
                                            <label for="">{IFSTRESS}</label>  
                                            <input type="text" {READONLY_IP} required class="inputs" name="stress" id="rg_stress" placeholder="{RPSTRESS}" valueold="{STRESSOLD}" value="{STRESSUSER}" />
                                            <!-- <button type="button" class="getstressold">{BUTTONSTRESS}</button> --> 
                                        </div>
                                        <div class="txt-fld">
                                            <label for="">{IFREGION}</label>
                                            <input type="text" {READONLY_IP} required class="inputs" name="region" id="rg_region" placeholder="{RPREGION}" value="{REGIONUSER}"/>
                                        </div>
                                        <div class="txt-fld">
                                            <label for="">{IFCOMPANY}</label> <input type="text" required class="inputs" name="company" id="rg_company" placeholder="{RPCOMPANY}" value="{COMPANYUSER}" />
                                        </div>
                                        <div class="txt-fld">
                                            <label for=""> {IFPHONE}</label>
                                            <input type="number" required class="inputs" name="phone" id="rg_phone" placeholder="{RPPHONE}" value="{PHONEUSER}" />
                                        </div>
                                        <div class="txt-fld">
                                            <label for="">{IFNOTE}</label> 
                                            <textarea name="note" id="rg_note" class="inputs" placeholder="{RPNOTE}">{NOTEUSER}</textarea>
                                        </div>
                                    </div>

						            <input type="submit" value="{IFBUTTONPAY}" class="sm-register" style="width:360px; height:50px; background:url(style/images/icon-8.png) no-repeat; border:none; margin-left:20%; margin-top:50px; color:white; text-transform:uppercase; font-size:18px; cursor:pointer;"/><br><br>
						            
