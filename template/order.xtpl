						            <div id="form-left"> 
                                        <div class="txt-fld">
                                            <label for="">{IFEMAIL}</label>
                                            <input type="email" {READONLY_IP} class="inputs" required name="email" id="rg_email"  placeholder="{RPMAIL}" value="{EMAILUSER}" />
                                        </div> 
                                        <div class="txt-fld">
                                             <label for="">{ODNAME}</label>
                                             <input type="text" required class="inputs" name="name" id="rg_lastname" value="{NAMEUSER}" placeholder="{RPFIRSTNAME}{RPLASTNAME}" />
